package api;

/**
 *
 * @author bamboo3892
 */
public interface ILearnableAI {

	public void setMetaFromSavedObject(Object o);

	public boolean canLearnFrom(AISheet ai);

	/**
	 * Learn from otherAI
	 * @param otherAI
	 * @return message
	 */
	public String learnFrom(AISheet otherAI);

	public Object getSaveObject();

}
