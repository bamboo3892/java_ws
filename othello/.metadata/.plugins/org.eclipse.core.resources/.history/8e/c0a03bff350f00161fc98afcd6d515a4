package main;

public class Test {

	public static void main(String args[]) {
		int[][] box = new int[8][8];
		a(box);
		int[][] clone = box.clone();
		System.out.println(box == clone);
		System.out.println(box.equals(clone));
		print(box);
		print(clone);
	}

	public static void a(int[][] box) {
		box[4][4] = 1;
	}

	public static void print(int[][] box) {
		for (int x = 0; x < 8; x++){
			for (int y = 0; y < 8; y++){
				System.out.print(box[x][y]);
			}
			System.out.println("");
		}
	}

}
