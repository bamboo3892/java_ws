package api;

/**
 *
 * @author bamboo3892
 */
public interface ILearnableAI {

	public void setMetaFromSavedObject(Object o, boolean willLearn);

	public boolean canLearnFrom(AISheet ai);

	/**
	 *
	 * @param otherAI
	 * @return message
	 */
	public String learn(AISheet otherAI);

	public Object getSaveObject();

}
