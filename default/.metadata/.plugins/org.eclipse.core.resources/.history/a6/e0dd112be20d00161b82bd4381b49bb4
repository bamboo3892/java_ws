package ai;


public class AdvancedEstimateAI extends EstimateAI{

	public AdvancedEstimateAI(){
		setGeneFromMeta();
	}

	public AdvancedEstimateAI(boolean b){
		if(b){
			setGeneFromMeta();
		}else{
			setNextGene();
		}
	}

	@Deprecated
	public AdvancedEstimateAI(int[][] box, int teban) {
		super(box, teban);
		/*
		originalTeban = teban;
		changeList(box, 0);
		*/
	}

	@Override
	protected float estimate(int[][] pbox, int pteban){

		float value = super.estimate(pbox, pteban);
		;
		;
		;
		;
		;
		;
		;
		//System.out.println("assessment value is " + value);
		return value;
	}

}
